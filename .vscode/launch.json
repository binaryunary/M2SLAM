{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/install/lib/orbslam_client/orbslam_client_node_mono_kitti",
      "args": ["${workspaceFolder}/src/orbslam_client/Vocabulary/ORBvoc.txt", "${workspaceFolder}/testbench/kitti-mono.yaml", "/dsets/KITTI_synced_rectified/2011_09_26/2011_09_26_drive_0009_sync/image_00"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [{"name": "LD_LIBRARY_PATH", "value": "/opt/ros/melodic/lib:${workspaceFolder}/install/lib"}],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          },
          {
              "description": "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "ROS: Attach",
      "runtime": "C++",
      "request": "attach",
      "type": "ros",
    },
  ]
}
